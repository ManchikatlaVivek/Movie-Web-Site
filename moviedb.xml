        <pma:database name="movie" collation="latin1_swedish_ci" charset="latin1">
            <pma:table name="actor">
                CREATE TABLE `actor` (
                  `actor_id` varchar(30) NOT NULL,
                  `fname` varchar(30) DEFAULT NULL,
                  `lname` varchar(30) DEFAULT NULL,
                  `dob` date DEFAULT NULL,
                  `no_of_awards` int(11) DEFAULT NULL,
                  PRIMARY KEY (`actor_id`)
                ) ENGINE=InnoDB DEFAULT CHARSET=latin1;
            </pma:table>
            <pma:table name="awards">
                CREATE TABLE `awards` (
                  `awardname` varchar(30) NOT NULL DEFAULT '',
                  `movie_id` varchar(30) NOT NULL DEFAULT '',
                  `year` varchar(4) DEFAULT NULL,
                  `category` varchar(30) NOT NULL DEFAULT '',
                  `type` varchar(40) NOT NULL,
                  PRIMARY KEY (`awardname`,`movie_id`,`category`)
                ) ENGINE=InnoDB DEFAULT CHARSET=latin1;
            </pma:table>
            <pma:table name="director">
                CREATE TABLE `director` (
                  `director_id` varchar(30) NOT NULL,
                  `fname` varchar(30) DEFAULT NULL,
                  `lname` varchar(30) DEFAULT NULL,
                  `dob` date DEFAULT NULL,
                  `gender` char(1) DEFAULT NULL,
                  PRIMARY KEY (`director_id`)
                ) ENGINE=InnoDB DEFAULT CHARSET=latin1;
            </pma:table>
            <pma:table name="movie">
                CREATE TABLE `movie` (
                  `movie_id` varchar(30) NOT NULL,
                  `actor_id` varchar(30) DEFAULT NULL,
                  `director_id` varchar(30) DEFAULT NULL,
                  `title` varchar(30) DEFAULT NULL,
                  `genre` varchar(30) DEFAULT NULL,
                  `year` varchar(4) DEFAULT NULL,
                  `rating` float DEFAULT NULL,
                  `runtime` int(11) DEFAULT NULL,
                  `collections` int(11) DEFAULT NULL,
                  PRIMARY KEY (`movie_id`)
                ) ENGINE=InnoDB DEFAULT CHARSET=latin1;
            </pma:table>
            <pma:table name="WISHLIST">
                CREATE TABLE `WISHLIST` (
                  `USERNAME` varchar(20) NOT NULL,
                  `TITLE` varchar(20) NOT NULL,
                  PRIMARY KEY (`USERNAME`,`TITLE`)
                ) ENGINE=InnoDB DEFAULT CHARSET=latin1;
            </pma:table>
        </pma:database>
    </pma:structure_schemas>

